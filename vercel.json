{
  "framework": "nextjs",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://surfbreak.ru"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://surfbreak.ru;"
        }
      ]
    }
  ]
} 
